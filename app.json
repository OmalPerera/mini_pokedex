{
  "expo": {
    "name": "PokePals",
    "slug": "PokePals",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "minipokedex",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "splash": {
        "image": "./assets/images/splash_unwink.png",
        "resizeMode": "contain",
        "backgroundColor": "#D8F0F7"
      },
      "bundleIdentifier": "com.omal.pokepals"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#D8F0F7"
      },
      "splash": {
        "image": "./assets/images/splash_unwink.png",
        "resizeMode": "contain",
        "backgroundColor": "#D8F0F7"
      },
      "package": "com.omal.minipokedex"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash_unwink.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#D8F0F7",
          "dark": {
            "backgroundColor": "#D8F0F7"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
